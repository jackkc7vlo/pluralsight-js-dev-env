{
   "root": true,
   "extends": [
     "eslint:recommended",
     "plugin:import/errors",
     "plugin:import/warnings"
   ],
   "parserOptions": {
     "ecmaVersion": 2020,
     "sourceType": "module"
    },
   "env": {
     "browser": true,
     "node": true,
     "mocha": true
   },
   "rules": {
      "curly" :"error",
      "eqeqeq" :"error",
      "brace-style" :"error",
      "keyword-spacing":"error",
      "new-cap":"error",
      "new-parens":"error",
      "indent": ["error", 2],
      "no-console" : 1
   }
 }
